<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="34dp"
            android:height="24dp"
            android:viewportWidth="34"
            android:viewportHeight="24">
            <group
                android:name="alert"
                android:pivotX="17">
                <path
                    android:name="bell_shape"
                    android:pathData="M 28.925 19.973 C 28.719 20.587 28.14 21.005 27.491 21 L 19.994 21 C 19.994 21.008 18.996 21.008 17 21 L 6.501 21 C 5.856 21 5.281 20.587 5.076 19.974 C 4.873 19.362 5.085 18.687 5.601 18.3 C 7.116 17.171 8.01 15.39 8.008 13.5 L 8.008 9 C 8.008 4.029 12.037 0 17.005 0 C 21.973 0 26 4.029 26 9 L 26 13.5 C 25.995 15.39 26.885 17.171 28.4 18.3 C 28.916 18.687 29.126 19.36 28.925 19.973 Z M 23.249 13.508 L 23.249 9.008 C 23.121 5.647 20.363 2.993 17.003 2.993 C 13.644 2.993 10.884 5.647 10.758 9.008 L 10.758 13.508 C 10.758 15.093 10.326 16.648 9.508 18.008 L 24.5 18.008 C 23.682 16.65 23.25 15.093 23.249 13.508 Z"
                    android:fillColor="#000"
                    android:strokeWidth="1"/>
                <group android:name="ball">
                    <path
                        android:name="bell_ball"
                        android:pathData="M 17 18 C 15.343 18 14 19.343 14 21 C 14 22.657 15.343 24 17 24 C 18.657 24 20 22.657 20 21 C 20 19.343 18.657 18 17 18 Z"
                        android:fillColor="#000"
                        android:strokeWidth="1"/>
                </group>
            </group>
        </vector>
    </aapt:attr>
    <target android:name="alert">
        <aapt:attr name="android:animation">
            <set>

                <objectAnimator
                    android:propertyName="scaleX"
                    android:duration="142"
                    android:valueFrom="1"
                    android:valueTo="1.2"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>


                <objectAnimator
                    android:propertyName="scaleY"
                    android:duration="142"
                    android:valueFrom="1"
                    android:valueTo="1.2"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>



                <objectAnimator
                    android:propertyName="scaleX"
                    android:duration="142"
                    android:valueFrom="1"
                    android:valueTo="1.2"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>


                <objectAnimator
                    android:propertyName="scaleX"
                    android:duration="142"
                    android:valueFrom="1.2"
                    android:valueTo="1.0"
                    android:startOffset="142"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>


                <objectAnimator
                    android:propertyName="scaleY"
                    android:duration="142"
                    android:valueFrom="1.2"
                    android:valueTo="1.0"
                    android:startOffset="142"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>


                <objectAnimator
                    android:propertyName="rotation"
                    android:duration="142"
                    android:valueFrom="0"
                    android:valueTo="12"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="rotation"
                    android:startOffset="152"
                    android:duration="130"
                    android:valueFrom="12"
                    android:valueTo="-12"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="rotation"
                    android:startOffset="290"
                    android:duration="115"
                    android:valueFrom="-12"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/overshoot_interpolator"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="ball">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="translateX"
                    android:startOffset="67"
                    android:duration="100"
                    android:valueFrom="0"
                    android:valueTo="-3"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="translateX"
                    android:startOffset="167"
                    android:duration="100"
                    android:valueFrom="-3"
                    android:valueTo="3"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="translateX"
                    android:startOffset="301"
                    android:duration="100"
                    android:valueFrom="3"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/overshoot_interpolator"/>
            </set>
        </aapt:attr>
    </target>
</animated-vector>
